{
  "expo": {
    "scheme": "hhautoapply",
    "deepLinking": true,
    "platforms": ["ios", "android"],
    "name": "hh-autoapply-app",
    "slug": "hh-autoapply-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/aa-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.yourcompany.hhautoapply",
      "supportsTablet": true,
      "associatedDomains": ["applinks:hhautoapply.com"]
    },
    "android": {
      "permissions": ["ACCESS_BACKGROUND_LOCATION", "FOREGROUND_SERVICE"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      [
        "expo-task-manager"
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
